            fx:controller="client.controller.BookEditController">
    <GridPane layoutY="10.0" prefHeight="220.0" prefWidth="600.0" AnchorPane.leftAnchor="10.0"
              AnchorPane.topAnchor="10.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="512.0" minWidth="10.0" prefWidth="99.0"/>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="501.0" minWidth="0.0" prefWidth="501.0"/>
